{
    "name": "oli/autograph",
    "description": "Doctrine to GraphQL bridge",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "oligus",
            "email": "oli.gustafsson@gmail.com"
        }
    ],
    "minimum-stability": "stable",
    "config": {
        "platform": {
            "php": "7.4"
        }
    },
    "require": {
        "php": ">=7.4",
        "ext-json": "*",
        "doctrine/orm": "^2.6",
        "webonyx/graphql-php": "^0.13.8",
        "php-di/php-di": "^6.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.4",
        "symfony/var-dumper": "^5.0",
        "spatie/phpunit-snapshot-assertions": "^3.0",
        "oligus/doctrine-fixtures": "^0.1.5"
    },
    "autoload": {
        "psr-4": {
            "Autograph\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Autograph\\Tests\\": "tests/src/",
            "Autograph\\Tests\\E2E\\": "tests/e2e/",
            "Tests\\": "tests/",
            "Autograph\\Demo\\": "demo/"
        },
        "classmap": [
            "tests/"
        ]
    }
}
