{
    "name": "oli/autograph",
    "description": "Doctrine to GraphQL bridge",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "oligus",
            "email": "oli.gustafsson@gmail.com"
        }
    ],
    "require": {
        "ext-json": "*",
        "doctrine/orm": "^2.6",
        "webonyx/graphql-php": "^0.13.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^7.5",
        "symfony/var-dumper": "^4.2",
        "spatie/phpunit-snapshot-assertions": "^2.0"
    },
    "autoload": {
        "psr-4": {
            "Autograph\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Autograph\\Tests\\": "tests/src/",
            "Tests\\": "tests/",
            "Autograph\\Demo\\": "demo/"
        },
        "classmap": [
            "tests/"
        ]
    }
}
